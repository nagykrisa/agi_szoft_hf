{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "node",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect"
      ],
      "program": "${workspaceRoot}/index.js",
      "restart": true,
      "port": 9229,
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": { "NODE_ENV": "dev" }
    }
  ],
  "compounds": []
}
